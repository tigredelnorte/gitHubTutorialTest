package demo1;

public class Leet7 {
	public static void main(String[] args) {
		System.out.println(reverse(345));
		
	}
	 public static int reverse(int x) {
	        int xrev=x%10;
	        while(x>0){
	           xrev = xrev*10+(x%10);
	           x=x/10; 
	       }
	        return xrev;
	    }
}
