package demo1;

public class Leet7 {
	public static void main(String[] args) {
		System.out.println(reverse(-123));
		System.out.println(-123%10);
		System.out.println(-123/10);
	}
	 public static int reverse(int x) {
	        int xrev=0;
	        while(x/10!=0){
	           xrev = xrev*10+(x%10);
	           x=x/10; 
	       }
	        return xrev;
	    }
	 
}
