package test;

class Processor extends Thread{
	public void run() {
		while(true) {
			System.out.println("KURWA");
			
			try {
				Thread.sleep(150);
			} catch (InterruptedException e) {
			}
		}
	}
}

public class AppTest {
	public static void main(String[] args) {
		Processor p1 = new Processor();
		p1.start();

	}
}
